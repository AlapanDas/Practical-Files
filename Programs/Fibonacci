def fibonacci(n):
    if n<=1:
        return n
    return (fibonacci(n-1)+fibonacci(n-2))

n=int(input("Enter the Number of term - "))
print("The ",n,"th fibonacci term is",fibonacci(n-1))
def generatefibo(n):
    a=0
    b=1
    for i in range(1,n+1):
        c=a+b
        print(c)
        a=b
        b=c
n=int(input("ENTER LIMIT"))
generatefibo(n)